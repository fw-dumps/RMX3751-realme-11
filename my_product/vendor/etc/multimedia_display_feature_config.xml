<?xml version="1.0" encoding="UTF-8"?>

<filter-conf>
    <version>20230314</version>
    <toggle>1</toggle>
    <features>
        <feature name="BindCore" enable="1">
            <!-- big core index, start from 0 -->
            <bigcore_id>6</bigcore_id>
            <bigcore_id>7</bigcore_id>
        </feature>

        <feature name="DispKeventThread" enable="1"/>

        <feature name="hecate" enable="1"/>

        <feature name="OplusDEE" enable="1">
            <DC>enable</DC>
            <globalLTM>enable</globalLTM>
            <application name="com.coloros.video">
                <scene type="Application">
                    <action>
                        <mode>LTM</mode>
                    </action>
                </scene>
            </application>
            <application name="com.oplus.camera">
                <scene type="Application">
                    <action>
                        <mode>LTM</mode>
                    </action>
                </scene>
            </application>
            <application name="com.coloros.gallery3d">
                <scene type="Application">
                    <action>
                        <mode>LTM</mode>
                    </action>
                </scene>
            </application>
            <application name="com.oneplus.gallery">
                <scene type="Application">
                    <action>
                        <mode>LTM</mode>
                    </action>
                </scene>
            </application>
            <application name="com.heytap.yoli">
                <scene type="Application">
                    <action>
                        <mode>LTM</mode>
                    </action>
                </scene>
            </application>
        </feature>

        <feature name="osie" enable="1">
            <!-- osie start -->
            <!-- 快手 -->
            <application name="com.smile.gifmaker">
                <scene type="ExactLayer">
                    <layer>HomeActivity</layer>
                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>0</targetLayerIndex>
                    </action>
                </scene>
                <scene type="ExactLayer">
                    <layer>PhotoDetailActivity</layer>
                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>0</targetLayerIndex>
                    </action>
                </scene>
                <scene type="ExactLayer">
                    <layer>HomeActivity</layer>
                    <layer>PhotoDetailActivity</layer>
                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>0</targetLayerIndex>
                    </action>
                </scene>
                <scene type="ExactLayer">
                    <layer>ReviewActivity</layer>
                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>0</targetLayerIndex>
                    </action>
                </scene>
                <scene type="ExactLayer">
                    <layer>HomeActivity</layer>
                    <layer>PhotoDetailActivity</layer>
                    <layer>PhotoDetailActivity</layer>
                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>2</targetLayerIndex>
                    </action>
                </scene>
                <scene type="ExactLayer">
                    <layer>HomeActivity</layer>
                    <layer>HomeActivity</layer>
                    <layer>PhotoDetailActivity</layer>
                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>2</targetLayerIndex>
                    </action>
