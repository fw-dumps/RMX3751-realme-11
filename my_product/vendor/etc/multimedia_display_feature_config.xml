                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>2</targetLayerIndex>
                    </action>
                </scene>
                <scene type="ExactLayer">
                    <layer>HomeActivity</layer>
                    <layer>PhotoDetailActivity</layer>
                    <layer>UserProfileActivity</layer>
                    <layer>PhotoDetailActivity</layer>
                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>3</targetLayerIndex>
                    </action>
                </scene>
            </application>
            <!-- 微视 -->
            <application name="com.tencent.weishi">
                <scene type="ExactLayer">
                    <layer>MainActivity</layer>
                    <action>
                        <mode>PQ</mode>
                        <targetLayerIndex>0</targetLayerIndex>
                    </action>
                </scene>
                <scene type="ExactLayer">
                    <layer>MaterialDetailActivity</layer>
